        static int GetMax(int[] arr)
        {
            int max = int.MinValue;
            for (int i = 0; i < arr.Length; i++)
            {
                if (arr[i] > max)
                {
                    max = arr[i];
                }
            }
            return max;
        }

        static void Main(string[] args)
        {
            int[] arr = { -7, 5, 60, -33, 42 };
            Console.WriteLine($"최대값은 : {GetMax(arr)}");
        }
    }
