        static void Main(string[] args)
        {
            // 크기가 5인 정수형 배열 arrInt를 선언하고
            // 값은 10, 20, 30, 40, 50을 넣어보기
            int[] arrInt = new int[5];


            // 배열의 끝 = 공식으로 표현 시 N-1
            for (int i = 0; i<arrInt.Length; i++)
            {
                arrInt[i] = (i + 1) * 10;
            }
            for (int i = 0; i < arrInt.Length; i++)
            {
                Console.WriteLine(arrInt[i]);
            }
        }
