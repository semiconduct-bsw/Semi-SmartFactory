        static void Main(string[] args)
        {
            int[] arr = { -7, 5, 60, -33, 42 };
            int max = int.MinValue; // 정수 중 가장 작은 값을 max 변수에 대입함

            for (int i = 0; i<arr.Length; i++)
            {
                if (arr[i] > max)
                {
                    max = arr[i];
                }
            }

            Console.WriteLine($"최대값은 : {max}");
        }
