using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace _20240625_Framework
{
    class Program
    {
        static void Main(string[] args)
        {
            string greet = "반갑습니다.";
            string greet2 = "안녕하세요.";
            string name = "배선우";
            int age = 23;
            Console.WriteLine("{0} {1}", greet, greet2);
            Console.WriteLine($"저는 {name} {age}살 입니다.");
        }
    }
}
